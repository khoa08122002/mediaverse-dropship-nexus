{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist",
  "functions": {
    "api/comprehensive.js": {
      "memory": 1024,
      "maxDuration": 30
    },
    "api/backend.js": {
      "memory": 1024,
      "maxDuration": 30
    },
    "api/index.js": {
      "memory": 1024,
      "maxDuration": 30
    },
    "api/health.js": {
      "memory": 512,
      "maxDuration": 10
    },
    "api/test.js": {
      "memory": 512,
      "maxDuration": 10
    },
    "api/express.js": {
      "memory": 1024,
      "maxDuration": 30
    },
    "api/simple.js": {
      "memory": 512,
      "maxDuration": 10
    },
    "api/express-simple.js": {
      "memory": 512,
      "maxDuration": 10
    },
    "api/test-db.js": {
      "memory": 512,
      "maxDuration": 15
    },
    "api/init-admin.js": {
      "memory": 512,
      "maxDuration": 15
    },
    "api/ping.js": {
      "memory": 256,
      "maxDuration": 5
    }
  },
  "routes": [
    {
      "src": "/api/comprehensive/(.*)",
      "dest": "api/comprehensive.js"
    },
    {
      "src": "/api/comprehensive",
      "dest": "api/comprehensive.js"
    },
    {
      "src": "/api/backend/(.*)",
      "dest": "api/backend.js"
    },
    {
      "src": "/api/backend",
      "dest": "api/backend.js"
    },
    {
      "src": "/api/test-db",
      "dest": "api/test-db.js"
    },
    {
      "src": "/api/init-admin",
      "dest": "api/init-admin.js"
    },
    {
      "src": "/api/ping",
      "dest": "api/ping.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/_vercel/insights/script.js",
      "dest": "/_vercel/insights/script.js"
    },
    {
      "src": "/[^/]+\\.(js|mjs|ts|jsx|tsx|css|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot|webp|avif|mp4|webm|map|json)$",
      "dest": "/$0",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/robots.txt",
      "dest": "/robots.txt"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/((?!api|assets|_vercel).*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 