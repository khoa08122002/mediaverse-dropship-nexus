{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "functions": {
    "api/index.js": {
      "memory": 1024,
      "maxDuration": 30
    },
    "api/health.js": {
      "memory": 512,
      "maxDuration": 10
    },
    "api/test.js": {
      "memory": 512,
      "maxDuration": 10
    },
    "api/express.js": {
      "memory": 1024,
      "maxDuration": 30
    },
    "api/simple.js": {
      "memory": 512,
      "maxDuration": 10
    },
    "api/express-simple.js": {
      "memory": 512,
      "maxDuration": 10
    },
    "api/backend.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/backend/(.*)",
      "dest": "api/backend.js"
    },
    {
      "src": "/api/backend",
      "dest": "api/backend.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/test-routing.html",
      "dest": "public/test-routing.html"
    },
    {
      "src": "/assets/(.*)",
      "dest": "dist/assets/$1"
    },
    {
      "src": "/(favicon\\.ico|manifest\\.json|robots\\.txt|logo\\.png|avatar\\.jpg|placeholder\\.svg)",
      "dest": "dist/$1"
    },
    {
      "src": "/.*\\.(js|css|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot|mp4|webm|wav|mp3|m4a|aac|oga|map|txt|json)$",
      "dest": "dist/$0"
    },
    {
      "src": "/(.*)",
      "dest": "dist/index.html"
    }
  ],
  "outputDirectory": "dist",
  "env": {
    "NODE_ENV": "production"
  }
} 