{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "functions": {
    "api/index.js": {
      "memory": 1024,
      "maxDuration": 30
    },
    "api/health.js": {
      "memory": 512,
      "maxDuration": 10
    },
    "api/test.js": {
      "memory": 512,
      "maxDuration": 10
    },
    "api/express.js": {
      "memory": 1024,
      "maxDuration": 30
    },
    "api/simple.js": {
      "memory": 512,
      "maxDuration": 10
    },
    "api/express-simple.js": {
      "memory": 512,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/simple",
      "dest": "api/simple.js"
    },
    {
      "src": "/api/health",
      "dest": "api/health.js"
    },
    {
      "src": "/api/test",
      "dest": "api/test.js"
    },
    {
      "src": "/api/express/(.*)",
      "dest": "api/express.js"
    },
    {
      "src": "/api/express",
      "dest": "api/express.js"
    },
    {
      "src": "/api/express-simple/(.*)",
      "dest": "api/express-simple.js"
    },
    {
      "src": "/api/express-simple",
      "dest": "api/express-simple.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "dist/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 