{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist",
  "functions": {
    "api/comprehensive.js": {
      "memory": 1024,
      "maxDuration": 30
    },
    "api/backend.js": {
      "memory": 1024,
      "maxDuration": 30
    },
    "api/index.js": {
      "memory": 1024,
      "maxDuration": 30
    },
    "api/health.js": {
      "memory": 512,
      "maxDuration": 10
    },
    "api/test.js": {
      "memory": 512,
      "maxDuration": 10
    },
    "api/express.js": {
      "memory": 1024,
      "maxDuration": 30
    },
    "api/simple.js": {
      "memory": 512,
      "maxDuration": 10
    },
    "api/express-simple.js": {
      "memory": 512,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/comprehensive/(.*)",
      "dest": "api/comprehensive.js"
    },
    {
      "src": "/api/comprehensive",
      "dest": "api/comprehensive.js"
    },
    {
      "src": "/api/backend/(.*)",
      "dest": "api/backend.js"
    },
    {
      "src": "/api/backend",
      "dest": "api/backend.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/test-login.html",
      "dest": "dist/test-login.html"
    },
    {
      "src": "/test-routing.html",
      "dest": "dist/test-routing.html"
    },
    {
      "src": "/favicon.ico",
      "dest": "dist/favicon.ico"
    },
    {
      "src": "/robots.txt",
      "dest": "dist/robots.txt"
    },
    {
      "src": "/assets/(.*)",
      "dest": "dist/assets/$1"
    },
    {
      "src": "/static/(.*)",
      "dest": "dist/static/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot|mp4|webm|wav|mp3|m4a|aac|oga|map|txt|json))$",
      "dest": "dist/$1"
    },
    {
      "src": "/(login|register|forgot-password|about|contact|blog|recruitment|admin|media-services|ecommerce)/?",
      "dest": "dist/index.html",
      "headers": {
        "cache-control": "s-maxage=0"
      }
    },
    {
      "src": "/blog/(.*)",
      "dest": "dist/index.html",
      "headers": {
        "cache-control": "s-maxage=0"
      }
    },
    {
      "src": "/admin/(.*)",
      "dest": "dist/index.html",
      "headers": {
        "cache-control": "s-maxage=0"
      }
    },
    {
      "src": "/(.*)",
      "dest": "dist/index.html",
      "headers": {
        "cache-control": "s-maxage=0"
      }
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 