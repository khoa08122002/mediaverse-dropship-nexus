{
  "version": 2,
  "buildCommand": "npm run vercel-build",
  "functions": {
    "api/index.js": {
      "memory": 1024,
      "maxDuration": 30
    },
    "api/health.js": {
      "memory": 512,
      "maxDuration": 10
    },
    "api/test.js": {
      "memory": 512,
      "maxDuration": 10
    },
    "api/express.js": {
      "memory": 1024,
      "maxDuration": 30
    },
    "api/simple.js": {
      "memory": 512,
      "maxDuration": 10
    },
    "api/express-simple.js": {
      "memory": 512,
      "maxDuration": 10
    },
    "api/backend.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/simple",
      "dest": "api/simple.js"
    },
    {
      "src": "/api/health",
      "dest": "api/health.js"
    },
    {
      "src": "/api/test",
      "dest": "api/test.js"
    },
    {
      "src": "/api/express-simple/(.*)",
      "dest": "api/express-simple.js"
    },
    {
      "src": "/api/express-simple",
      "dest": "api/express-simple.js"
    },
    {
      "src": "/api/express/(.*)",
      "dest": "api/express.js"
    },
    {
      "src": "/api/express",
      "dest": "api/express.js"
    },
    {
      "src": "/api/backend/(.*)",
      "dest": "api/backend.js"
    },
    {
      "src": "/api/backend",
      "dest": "api/backend.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "dist/assets/$1"
    },
    {
      "src": "/(favicon\\.ico|manifest\\.json|robots\\.txt)",
      "dest": "dist/$1"
    },
    {
      "src": "/.*\\.(js|css|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot|mp4|webm|wav|mp3|m4a|aac|oga|map)$",
      "dest": "dist/$0"
    },
    {
      "src": "/(login|admin|blog|contact|about|recruitment|media-services|ecommerce)$",
      "dest": "dist/index.html",
      "headers": {
        "Content-Type": "text/html; charset=utf-8"
      }
    },
    {
      "src": "/blog/(.*)",
      "dest": "dist/index.html",
      "headers": {
        "Content-Type": "text/html; charset=utf-8"
      }
    },
    {
      "src": "/admin/(.*)",
      "dest": "dist/index.html",
      "headers": {
        "Content-Type": "text/html; charset=utf-8"
      }
    },
    {
      "src": "/(.*)",
      "dest": "dist/index.html",
      "headers": {
        "Content-Type": "text/html; charset=utf-8"
      }
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 