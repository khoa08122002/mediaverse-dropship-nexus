{
  "name": "phg-corporation",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently -k \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" vite --force",
    "dev:backend": "ts-node-dev --project tsconfig.backend.json --transpile-only src/backend/main.ts",
    "build": "npm run clean && npm run build:backend && npm run build:frontend",
    "build:frontend": "tsc -p tsconfig.frontend.json && vite build",
    "build:backend": "tsc -p tsconfig.backend.json",
    "postbuild:backend": "npm run prisma:generate && npm run copy-files",
    "copy-files": "copyfiles -u 1 \"prisma/schema.prisma\" dist/backend && copyfiles \".env\" dist/backend && npm install --prefix dist/backend --production",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "cd dist/backend && npm start",
    "start:prod": "cd dist/backend && cross-env NODE_ENV=production npm start",
    "start:dev": "npm run dev",
    "predeploy": "npm run prisma:deploy",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "vercel-build": "npm run build",
    "vercel-start": "npm run start:prod",
    "railway-build": "npm run build",
    "railway-start": "npm run start:prod",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:seed": "ts-node --project tsconfig.seed.json prisma/seed.ts",
    "clean": "rimraf dist",
    "postinstall": "npm run prisma:generate",
    "test:health": "cross-env NODE_ENV=production node scripts/test-health.js",
    "test:prod": "npm run build && npm run predeploy && npm run start:prod",
    "test:prod-health": "node scripts/test-prod-health.js"
  },
  "dependencies": {
    "@eslint/config-array": "^0.20.1",
    "@eslint/object-schema": "^2.1.6",
    "@hookform/resolvers": "^3.3.4",
    "@nestjs/common": "^10.3.3",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "^10.3.3",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.3",
    "@nestjs/swagger": "^7.3.0",
