{
  "name": "phg-corporation",
  "version": "1.0.0",
  "description": "Recruitment management system for PHG Corporation",
  "main": "src/index.ts",
  "scripts": {
    "dev:backend": "ts-node-dev --project tsconfig.backend.json --transpile-only src/backend/main.ts",
    "dev:frontend": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" vite --force",
    "dev": "concurrently -k \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build:frontend": "vite build",
    "build:backend": "tsc -p tsconfig.backend.json",
    "build": "npm run build:frontend",
    "vercel-build": "prisma generate && tsc -p tsconfig.backend.json && vite build",
    "start": "node dist/backend/main.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "preview": "vite preview",
    "clean": "rimraf dist node_modules/.vite"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.3",
    "@nestjs/core": "^10.3.3",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.3",
    "@prisma/client": "^5.10.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@splinetool/react-spline": "^2.2.6",
    "@tanstack/react-query": "^5.24.1",
    "@tanstack/react-query-devtools": "^5.24.1",
    "@tinymce/tinymce-react": "^6.2.1",
    "@types/cookie-parser": "^1.4.8",
    "@types/passport-local": "^1.0.38",
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.341.0",
    "mongoose": "^8.1.3",
    "multer": "1.4.5-lts.1",
    "next-auth": "^4.24.6",
    "passport-local": "^1.0.0",
    "prisma": "^5.10.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^7.6.2",
    "sonner": "^1.4.0",
    "tailwind-merge": "^2.2.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.57",
